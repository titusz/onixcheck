			<xs:enumeration value="US-TN">
				<xs:annotation>
					<xs:documentation>Tennessee</xs:documentation>
					<xs:documentation/>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="US-TX">
				<xs:annotation>
					<xs:documentation>Texas</xs:documentation>
