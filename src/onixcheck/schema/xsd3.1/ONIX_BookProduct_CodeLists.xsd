			<xs:enumeration value="US-WA">
				<xs:annotation>
					<xs:documentation>Washington</xs:documentation>
					<xs:documentation/>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="US-WI">
				<xs:annotation>
					<xs:documentation>Wisconsin</xs:documentation>
					<xs:documentation/>
				</xs:annotation>
