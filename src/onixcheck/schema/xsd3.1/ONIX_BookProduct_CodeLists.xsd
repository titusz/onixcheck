					<xs:documentation>South Carolina</xs:documentation>
					<xs:documentation/>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="US-SD">
				<xs:annotation>
					<xs:documentation>South Dakota</xs:documentation>
					<xs:documentation/>
				</xs:annotation>
