<grammar xmlns="http://relaxng.org/ns/structure/1.0" datatypeLibrary="http://www.w3.org/2001/XMLSchema-datatypes" ns="http://ns.editeur.org/onix/3.1/short" xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0" a:force-namespace-declaration="">
	<include href="ONIX_BookProduct_CodeLists.rng" />
	<include href="ONIX_XHTML_Subset.rng" />

	<start>
		<ref name="ONIXMessage" />
	</start>

	<define name="ONIXMessage">
		<element name="ONIXmessage">
			<optional><attribute name="refname"><value>ONIXMessage</value></attribute></optional>
			<optional><attribute name="shortname"><value>ONIXmessage</value></attribute></optional>
			<ref name="generalAttributes" />
			<ref name="releaseAttribute" />
			<group>
				<ref name="Header" />
				<choice>
					<ref name="NoProduct" />
					<oneOrMore><ref name="Product" /></oneOrMore>
				</choice>
			</group>
		</element>
	</define>

	<define name="Addressee">
		<element name="addressee">
			<optional><attribute name="refname"><value>Addressee</value></attribute></optional>
			<optional><attribute name="shortname"><value>addressee</value></attribute></optional>
			<ref name="generalAttributes" />
			<group>
				<choice>
					<group>
						<oneOrMore><ref name="AddresseeIdentifier" /></oneOrMore>
						<optional><ref name="AddresseeName" /></optional>
					</group>
					<ref name="AddresseeName" />
				</choice>
				<optional><ref name="ContactName" /></optional>
				<optional><ref name="TelephoneNumber" /></optional>
				<optional><ref name="EmailAddress" /></optional>
			</group>
		</element>
	</define>
	<define name="AddresseeIdentifier">
		<element name="addresseeidentifier">
			<optional><attribute name="refname"><value>AddresseeIdentifier</value></attribute></optional>
			<optional><attribute name="shortname"><value>addresseeidentifier</value></attribute></optional>
			<ref name="generalAttributes" />
			<group>
				<ref name="AddresseeIDType" />
				<optional><ref name="IDTypeName" /></optional>
				<ref name="IDValue" />
			</group>
		</element>
	</define>
	<define name="AddresseeIDType">
		<element name="m380">
			<optional><attribute name="refname"><value>AddresseeIDType</value></attribute></optional>
			<optional><attribute name="shortname"><value>m380</value></attribute></optional>
			<ref name="generalAttributes" />
			<ref name="List44" />
		</element>
	</define>
	<define name="AddresseeName">
		<element name="x300">
			<optional><attribute name="refname"><value>AddresseeName</value></attribute></optional>
			<optional><attribute name="shortname"><value>x300</value></attribute></optional>
			<ref name="generalAttributes" />
			<ref name="dt.NonEmptyString" />
		</element>
	</define>
	<define name="Affiliation">
		<element name="b046">
			<optional><attribute name="refname"><value>Affiliation</value></attribute></optional>
			<optional><attribute name="shortname"><value>b046</value></attribute></optional>
			<ref name="generalAttributes" />
			<ref name="languageAttribute" />
			<ref name="dt.NonEmptyString" />
		</element>
	</define>
	<define name="AffiliationIdentifier">
		<element name="affiliationidentifier">
			<optional><attribute name="refname"><value>AffiliationIdentifier</value></attribute></optional>
			<optional><attribute name="shortname"><value>affiliationidentifier</value></attribute></optional>
			<ref name="generalAttributes" />
			<group>
				<ref name="AffiliationIDType" />
				<optional><ref name="IDTypeName" /></optional>
				<ref name="IDValue" />
			</group>
		</element>
	</define>
	<define name="AffiliationIDType">
		<element name="x580">
			<optional><attribute name="refname"><value>AffiliationIDType</value></attribute></optional>
			<optional><attribute name="shortname"><value>x580</value></attribute></optional>
			<ref name="generalAttributes" />
			<ref name="List44" />
		</element>
	</define>
	<define name="AgentIdentifier">
		<element name="agentidentifier">
			<optional><attribute name="refname"><value>AgentIdentifier</value></attribute></optional>
			<optional><attribute name="shortname"><value>agentidentifier</value></attribute></optional>
			<ref name="generalAttributes" />
			<group>
				<ref name="AgentIDType" />
				<optional><ref name="IDTypeName" /></optional>
				<ref name="IDValue" />
			</group>
		</element>
	</define>
	<define name="AgentIDType">
		<element name="j400">
			<optional><attribute name="refname"><value>AgentIDType</value></attribute></optional>
			<optional><attribute name="shortname"><value>j400</value></attribute></optional>
			<ref name="generalAttributes" />
			<ref name="List92" />
		</element>
	</define>
	<define name="AgentName">
		<element name="j401">
			<optional><attribute name="refname"><value>AgentName</value></attribute></optional>
			<optional><attribute name="shortname"><value>j401</value></attribute></optional>
			<ref name="generalAttributes" />
			<ref name="languageAttribute" />
			<ref name="collationkeyAttribute" />
			<ref name="dt.NonEmptyString" />
		</element>
	</define>
	<define name="AgentRole">
		<element name="j402">
			<optional><attribute name="refname"><value>AgentRole</value></attribute></optional>
			<optional><attribute name="shortname"><value>j402</value></attribute></optional>
			<ref name="generalAttributes" />
			<ref name="List69" />
		</element>
	</define>
	<define name="AlternativeName">
		<element name="alternativename">
			<optional><attribute name="refname"><value>AlternativeName</value></attribute></optional>
			<optional><attribute name="shortname"><value>alternativename</value></attribute></optional>
			<ref name="generalAttributes" />
			<group>
				<ref name="NameType" />
				<choice>
					<group>
						<oneOrMore><ref name="NameIdentifier" /></oneOrMore>
						<optional>
							<choice>
								<group>
									<ref name="gp.unstructured_name" />
									<optional><ref name="gp.structured_name" /></optional>
								</group>
								<ref name="gp.structured_name" />
								<ref name="gp.corporate_name" />
							</choice>
						</optional>
					</group>
					<group>
						<ref name="gp.unstructured_name" />
						<optional><ref name="gp.structured_name" /></optional>
					</group>
					<ref name="gp.structured_name" />
					<ref name="gp.corporate_name" />
				</choice>
			</group>
		</element>
	</define>
	<define name="AncillaryContent">
		<element name="ancillarycontent">
			<optional><attribute name="refname"><value>AncillaryContent</value></attribute></optional>
			<optional><attribute name="shortname"><value>ancillarycontent</value></attribute></optional>
			<ref name="generalAttributes" />
			<group>
				<ref name="AncillaryContentType" />
				<zeroOrMore><ref name="AncillaryContentDescription" /></zeroOrMore>
				<optional><ref name="Number" /></optional>
			</group>
		</element>
	</define>
	<define name="AncillaryContentType">
		<element name="x423">
			<optional><attribute name="refname"><value>AncillaryContentType</value></attribute></optional>
			<optional><attribute name="shortname"><value>x423</value></attribute></optional>
			<ref name="generalAttributes" />
			<ref name="List25" />
		</element>
	</define>
	<define name="AncillaryContentDescription">
		<element name="x424">
			<optional><attribute name="refname"><value>AncillaryContentDescription</value></attribute></optional>
			<optional><attribute name="shortname"><value>x424</value></attribute></optional>
			<ref name="generalAttributes" />
			<ref name="languageAttribute" />
			<ref name="textformatAttribute" />
			<ref name="Flow" />
		</element>
	</define>
	<define name="Audience">
		<element name="audience">
			<optional><attribute name="refname"><value>Audience</value></attribute></optional>
			<optional><attribute name="shortname"><value>audience</value></attribute></optional>
